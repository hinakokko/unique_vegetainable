<h1>レビュー投稿フォーム</h1>
<%= form_with model: @post_review do |f| %>
  <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>
  <h4>農家</h4>
  <%= f.text_field :farmer_name %>
  <h4>販売店舗</h4>
  <%= f.text_field :shop_name %>
  <h4>説明</h4>
  <%= f.text_area :caption %>
  <%= f.submit '投稿' %>
<% end %>